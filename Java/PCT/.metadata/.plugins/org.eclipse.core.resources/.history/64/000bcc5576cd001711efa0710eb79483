package javaapplication2;

public class BinaryTree {
	static Node root;
	
	public static void main(String[] args) {
		
	}
	
	
	public static void addNode(int nodeValue) {
		
		if(root == null)
			root = new Node(nodeValue);
		
		if(nodeValue > root.Key)
			root = root.right;
		

		if(nodeValue < root.Key)
			root = root.right;
		
	}
}
