class Node {
	public int value;
	public Nodewa lc;
	public Nodewa rc;
	public int level;
	public Nodewa pr;

	public Nodewa(int val, int lvl, Nodewa parent) {
		this.value = val;
		this.level = lvl;
		this.pr = parent;
	}
}